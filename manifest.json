{

    "manifest_version": 2,
    "name": "Dataset Extension",
    "version": "1.0.0",
  
    "description": "A cross browser extension to create image datasets by labelling and organizing images on the internet",
  
    "icons": {
      "48": "src/assets/icons/48.png"
    },

    "permissions": [
        "activeTab"
    ],
  
    "browser_action": {
        "default_icon": "src/assets/icons/32.png",
        "default_title": "Dataset Extension",
        "default_popup": "src/browser-actions/popup/index.html"
      },
    
      "web_accessible_resources": [
        "src/toolbar/index.html"
      ]
    
  
  }